<?xml version="1.0"?>
<config>
    <modules>
        <Rp_Cmsnewtab>
            <version>0.1.0</version>
        </Rp_Cmsnewtab>
    </modules>
    <global>
        <models>
            <cmsnewtab>
                <class>Rp_Cmsnewtab_Model</class>
            </cmsnewtab>
        </models>
        <events>
            <adminhtml_cms_page_edit_tab_main_prepare_form>
                <observers>
                    <rp_page_edit_tab_main>
                        <type>singleton</type>
                        <class>Rp_Cmsnewtab_Model_Observer</class>
                        <method>cmsField</method>
                    </rp_page_edit_tab_main>
                </observers>
            </adminhtml_cms_page_edit_tab_main_prepare_form>
        </events>
        <resources>
            <cmsnewtab_setup>
                <setup>
                    <module>Rp_Cmsnewtab</module>
                </setup>
                <connection>
                    <use>core_setup</use>
                </connection>
            </cmsnewtab_setup>
            <cmsnewtab_write>
                <connection>
                    <use>core_write</use>
                </connection>
            </cmsnewtab_write>
            <cmsnewtab_read>
                <connection>
                    <use>core_read</use>
                </connection>
            </cmsnewtab_read>
        </resources>
    </global>
</config>